{
  "name": "gallery",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "yarn fix-vips-concurrency-env && next dev --turbo --port \"$(dotenvx get GALLERY_PORT -f ../../.env)\"",
    "build": "next build",
    "start": "yarn fix-vips-concurrency-env && next start",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --max-warnings 0",
    "fix-vips-concurrency-env": "VIPS_CONCURRENCY=$(nproc 2>/dev/null || sysctl -n hw.logicalcpu)"
  },
  "dependencies": {
    "@storybook/react": "^8.2.7",
    "@valley/ui": "*",
    "axios": "^1.7.3",
    "axios-retry": "^4.5.0",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.12",
    "framer-motion": "^11.3.30",
    "geist": "^1.3.1",
    "geist-ui-icons": "^1.1.1",
    "immer": "^10.1.1",
    "jwt-decode": "^4.0.0",
    "next": "^14.2.7",
    "overlayscrollbars": "^2.10.0",
    "overlayscrollbars-react": "^0.5.6",
    "pretty-bytes": "^6.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.52.2",
    "react-textarea-autosize": "^8.5.3",
    "sharp": "^0.33.5",
    "swr": "^2.2.5",
    "vaul": "^0.9.6",
    "@dotenvx/dotenvx": "^1.14.2",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@csstools/postcss-media-minmax": "^2.0.1",
    "@next/eslint-plugin-next": "^14.2.7",
    "@swc/helpers": "^0.5.12",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "@valley/db": "*",
    "@valley/eslint-config": "*",
    "@valley/shared": "*",
    "@valley/typescript-config": "*",
    "eslint": "^8.56.11",
    "typescript": "^5.3.3",
    "typescript-plugin-css-modules": "^5.1.0"
  }
}
