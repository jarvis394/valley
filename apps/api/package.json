{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "yarn fix-vips-concurrency-env && nest start --watch",
    "build": "nest build",
    "start": "yarn fix-vips-concurrency-env && nest start",
    "start:debug": "yarn fix-vips-concurrency-env && nest start --debug --watch",
    "start:prod": "yarn fix-vips-concurrency-env && node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "fix-vips-concurrency-env": "VIPS_CONCURRENCY=$(nproc 2>/dev/null || sysctl -n hw.logicalcpu)"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.623.0",
    "@aws-sdk/lib-storage": "^3.645.0",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.3.10",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.3.10",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@valley/shared": "*",
    "bcryptjs": "^2.4.3",
    "cache-manager": "^5.7.6",
    "cache-manager-redis-yet": "^5.1.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.6",
    "@laosb/exifr": "^7.2.1",
    "lodash.deburr": "^4.1.0",
    "nestjs-prisma": "^0.23.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "sharp": "^0.33.5",
    "transliteration": "^2.3.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.7.5",
    "@swc/helpers": "^0.5.12",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.0",
    "@valley/eslint-config": "*",
    "@valley/typescript-config": "*",
    "eslint": "^8.56.11",
    "jest": "^29.5.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
