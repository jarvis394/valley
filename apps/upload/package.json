{
  "name": "upload",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=production node ./build/bin/server.js",
    "build": "cross-env NODE_ENV=production node ace build",
    "dev": "cross-env NODE_ENV=development node ace serve --hmr",
    "lint": "eslint . --max-warnings 0 --cache --cache-location ./node_modules/.cache/eslint",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "dotenv": "dotenvx run -f ../../.env --"
  },
  "imports": {
    "#controllers/*": "./app/controllers/*.js",
    "#exceptions/*": "./app/exceptions/*.js",
    "#models/*": "./app/models/*.js",
    "#mails/*": "./app/mails/*.js",
    "#services/*": "./app/services/*.js",
    "#listeners/*": "./app/listeners/*.js",
    "#events/*": "./app/events/*.js",
    "#middleware/*": "./app/middleware/*.js",
    "#validators/*": "./app/validators/*.js",
    "#providers/*": "./providers/*.js",
    "#policies/*": "./app/policies/*.js",
    "#abilities/*": "./app/abilities/*.js",
    "#utils/*": "./app/utils/*.js",
    "#lib/*": "./app/lib/*.js",
    "#types/*": "./app/types/*.js",
    "#jobs/*": "./app/jobs/*.js",
    "#database/*": "./database/*.js",
    "#start/*": "./start/*.js",
    "#config/*": "./config/*.js"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.8.2",
    "@adonisjs/eslint-config": "^2.0.0-beta.6",
    "@adonisjs/prettier-config": "^1.4.0",
    "@adonisjs/tsconfig": "^1.4.0",
    "@swc/core": "1.7.26",
    "@types/content-disposition": "^0.5.8",
    "@types/lodash.deburr": "^4.1.9",
    "@types/node": "^22.7.5",
    "@valley/eslint-config": "*",
    "@valley/typescript-config": "*",
    "eslint": "^9.12.0",
    "hot-hook": "^0.3.1",
    "pino-pretty": "^11.2.2",
    "prettier": "^3.3.3",
    "ts-node-maintained": "^10.9.4",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@adonisjs/auth": "^9.2.3",
    "@adonisjs/core": "^6.14.1",
    "@adonisjs/cors": "^2.2.1",
    "@adonisjs/drive": "^3.2.0",
    "@adonisjs/limiter": "^2.3.2",
    "@adonisjs/lock": "^1.1.1",
    "@adonisjs/redis": "^9.1.0",
    "@adonisjs/session": "^7.5.0",
    "@aws-sdk/client-s3": "^3.700.0",
    "@aws-sdk/lib-storage": "^3.701.0",
    "@aws-sdk/s3-request-presigner": "^3.700.0",
    "@dotenvx/dotenvx": "^1.14.2",
    "@google-cloud/storage": "^7.14.0",
    "@laosb/exifr": "^7.2.1",
    "@prisma/adapter-pg": "^6.0.1",
    "@prisma/client": "^6.0.1",
    "@rlanz/bull-queue": "^3.1.0",
    "@valley/db": "*",
    "@valley/shared": "*",
    "@vinejs/vine": "^3.0.0",
    "content-disposition": "^0.5.4",
    "cron": "^3.2.1",
    "cross-env": "^7.0.3",
    "lodash.deburr": "^4.1.0",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.2",
    "sharp": "^0.33.5",
    "transliteration": "^2.3.5",
    "uuid": "^11.0.3"
  },
  "hotHook": {
    "boundaries": [
      "./app/controllers/**/*.ts",
      "./app/middleware/*.ts"
    ]
  }
}
