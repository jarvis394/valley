{
  "extends": "@valley/typescript-config/remix.json",
  "include": [
    ".eslintrc.cjs",
    "**/*.ts",
    "**/*.tsx",
    "**/.server/**/*.ts",
    "**/.server/**/*.tsx",
    "**/.client/**/*.ts",
    "**/.client/**/*.tsx",
    "vite.config.js",
    ".react-router/types/**/*"
  ],
  "exclude": [".react-router/types/+virtual.ts"],
  "compilerOptions": {
    "baseUrl": ".",
    "rootDirs": [".", "./.react-router/types"]
  }
}
