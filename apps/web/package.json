    "build": "cross-env NODE_ENV=production remix vite:build && tsc --project ./tsconfig.server.json",
    "dev": "cross-env NODE_ENV=development tsx watch --clear-screen=false --ignore \".cache/**\" --ignore \"app/**\" --ignore \"vite.config.ts.timestamp-*\" --ignore \"build/**\" --ignore \"node_modules/**\" --inspect ./server/index.ts",
