{
  "name": "web",
  "version": "0.0.1",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_ENV=production remix vite:build && tsc --project ./tsconfig.server.json",
    "dev": "cross-env NODE_ENV=development tsx watch --clear-screen=false --ignore \".cache/**\" --ignore \"app/**\" --ignore \"vite.config.ts.timestamp-*\" --ignore \"build/**\" --ignore \"node_modules/**\" --inspect ./server/index.ts",
    "remix:dev": "yarn dotenv remix vite:dev",
    "start": "cross-env NODE_ENV=production node ./build/runner/index.js",
    "lint": "eslint . --max-warnings 0 --ignore-path ../../.gitignore --cache --cache-location ./node_modules/.cache/eslint",
    "typecheck": "tsc --noEmit",
    "deploy:cf": "npx wrangler pages deploy ./build/client",
    "dotenv": "dotenvx run -f ../../.env --"
  },
