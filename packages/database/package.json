{
  "name": "@valley/db",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "scripts": {
    "db:introspect": "drizzle-kit introspect --config=drizzle.config.ts",
    "db:generate": "drizzle-kit generate --config=drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config=drizzle.config.ts",
    "db:check": "drizzle-kit check --config=drizzle.config.ts",
    "db:up": "drizzle-kit up --config=drizzle.config.ts",
    "db:export": "drizzle-kit export --config=drizzle.config.ts",
    "db:push": "drizzle-kit push --config=drizzle.config.ts",
    "db:drop": "drizzle-kit drop --config=drizzle.config.ts",
    "db:studio": "drizzle-kit studio --config=drizzle.config.ts --port 8090",
    "dev": "swc src/* --watch --out-dir dist",
    "build": "tsc -b -v",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --max-warnings 0"
  },
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "files": [
    "./dist/**"
  ],
  "publishConfig": {
    "access": "public"
  },
  "typesVersions": {
    "*": {
      "*": [
        "src/*"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./dist/src/index.js",
      "require": "./dist/src/index.js"
    },
    "./*": {
      "types": "./src/*.ts",
      "import": "./dist/src/*.js",
      "require": "./dist/src/*.js"
    }
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.39.0",
    "@neondatabase/serverless": "^0.10.4",
    "@paralleldrive/cuid2": "^2.2.2",
    "drizzle-orm": "^0.40.1",
    "pg": "^8.13.3",
    "postgres": "^3.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "devDependencies": {
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.11.11",
    "@swc/helpers": "^0.5.15",
    "@types/node": "^22.9.0",
    "@types/pg": "^8.11.11",
    "drizzle-kit": "^0.30.5",
    "eslint": "^8.56.11"
  }
}
