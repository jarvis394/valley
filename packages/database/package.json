{
  "name": "@valley/db",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "db:generate": "bun db-env prisma generate --no-hints",
    "db:deploy": "bun db:generate && bun db-env prisma migrate deploy && bun db:seed",
    "db:migrate": "bun db-env prisma migrate dev",
    "db:migrate:reset": "bun db-env prisma migrate reset",
    "db:studio": "bun db-env prisma studio",
    "db:seed": "bun db-env ts-node prisma/seed.ts",
    "db-env": "dotenvx run -f ../../.env --",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --max-warnings 0"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "exports": {
    ".": "./src/index.ts"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.14.2",
    "@prisma/client": "^6.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "devDependencies": {
    "@types/node": "^22.9.0",
    "eslint": "^8.56.11",
    "prisma": "^6.4.1"
  }
}
