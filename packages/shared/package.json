{
  "name": "@valley/shared",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "swc src/* --watch --out-dir dist",
    "build": "tsc -b -v",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --max-warnings 0"
  },
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "files": [
    "./dist/**"
  ],
  "publishConfig": {
    "access": "public"
  },
  "typesVersions": {
    "*": {
      "*": [
        "src/*"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./dist/src/index.js",
      "require": "./dist/src/index.js"
    },
    "./*": {
      "types": "./src/*.ts",
      "import": "./dist/src/*.js",
      "require": "./dist/src/*.js"
    }
  },
  "devDependencies": {
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.7.5",
    "@swc/helpers": "^0.5.12",
    "@types/node": "^20.3.1",
    "@valley/db": "workspace:*",
    "@valley/eslint-config": "workspace:*",
    "@valley/typescript-config": "workspace:*",
    "eslint": "^8.56.11",
    "typescript": "^5.7.2"
  }
}
